<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<filter>
		<filter-name>EncodingFilter</filter-name>
		<filter-class>cn.e21.hbjyhf.web.utils.EncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
  		<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter>
		<filter-name>AutoLoginFilter</filter-name>
		<filter-class>cn.e21.hbjyhf.web.utils.AutoLoginFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>EncodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AutoLoginFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
  	<servlet>
		<servlet-name>InitBeanFactoryServlet</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.InitBeanFactoryServlet</servlet-class>
		<init-param>
			<param-name>configLocation</param-name>
			<param-value>beans.properties</param-value>
		</init-param>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>AreaAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.AreaAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SourceAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.SourceAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DiplomaAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.DiplomaAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ComplaintAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.ComplaintAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChartAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.ChartAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.LoginAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CollegeAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.CollegeAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CountryAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.CountryAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProvinceAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.ProvinceAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.UserAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReplyAction</servlet-name>
		<servlet-class>cn.e21.hbjyhf.web.action.ReplyAction</servlet-class>
	</servlet>
  <servlet>
  		<servlet-name>InformationAction</servlet-name>
  		<servlet-class>cn.e21.hbjyhf.web.action.InformationAction</servlet-class>
  	</servlet>
  <servlet> 
    <servlet-name>CityAction</servlet-name> 
    <servlet-class>cn.e21.hbjyhf.web.action.CityAction</servlet-class>
  </servlet>
  
<servlet-mapping>
    <servlet-name>CollegeAction</servlet-name>
    <url-pattern>/Servlet/CollegeAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SourceAction</servlet-name>
    <url-pattern>/Servlet/SourceAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DiplomaAction</servlet-name>
    <url-pattern>/Servlet/DiplomaAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ComplaintAction</servlet-name>
    <url-pattern>/Servlet/ComplaintAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChartAction</servlet-name>
    <url-pattern>/Servlet/ChartAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AreaAction</servlet-name>
    <url-pattern>/Servlet/AreaAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReplyAction</servlet-name>
    <url-pattern>/Servlet/ReplyAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProvinceAction</servlet-name>
    <url-pattern>/Servlet/ProvinceAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CountryAction</servlet-name>
    <url-pattern>/Servlet/CountryAction</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
    <servlet-name>LoginAction</servlet-name>
    <url-pattern>/Servlet/LoginAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InformationAction</servlet-name>
    <url-pattern>/Servlet/InformationAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CityAction</servlet-name>
    <url-pattern>/Servlet/CityAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserAction</servlet-name>
    <url-pattern>/Servlet/UserAction</url-pattern>
  </servlet-mapping>
  
	 
  
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
